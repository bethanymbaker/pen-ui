myList = [ "a", "b", "c", "d", "e" ]

idx = 0
target = 3
loop(item : myList)
  if (idx == 0 or idx == (myList.n() - 1))
    print("First or last! " + item)
  else if (idx == target and idx < (myList.n() - 1))
    print("Found target item (" + item + "), and it is not the last one!")
  else
    print("Middle item. " + item)
  done
  idx = idx + 1
done
